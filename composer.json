{
  "name": "babenoff/ld2",
  "description": "Text online game script",
  "minimum-stability": "stable",
  "version": "v0.0.1-alpha1",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Maks Babenko",
      "email": "mb.babenoff@yandex.ru"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/dependency-injection": "3.1.x-dev",
    "symfony/config": "3.1.x-dev",
    "symfony/routing": "3.1.x-dev",
    "symfony/http-foundation": "3.1.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*",
    "phpunit/php-invoker": "*",
    "phpunit/dbunit": ">=1.2"
  },
  "autoload": {
    "psr-4": {
      "LD2\\": [
        "lib/"
      ],
      "LD2Controller\\": [
        "game/", "site/", "errors/"
      ],
      "Test\\": [
        "tests/"
      ]
    }
  }
}
